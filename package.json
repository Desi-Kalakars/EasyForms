{
  "name": "easyforms",
  "version": "1.0.0",
  "scripts": {
    "start:backend": "cd backend && npm run start:dev",
    "start:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "cd backend && npm run build && cd ../frontend && npm run build"
  },
